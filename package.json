{
    "name": "mm",
    "version": "1.0.0",
    "dependencies": {
        "prop-types": "15.5.10",
        "react": "15.6.1",
        "react-dom": "15.6.1",
        "react-redux": "5.0.6",
        "redux": "3.7.2"
    },
    "devDependencies": {
        "@storybook/cli": "3.1.6",
        "@storybook/react": "3.1.8",
        "babel-cli": "6.24.1",
        "babel-loader": "7.1.1",
        "babel-plugin-add-module-exports": "0.2.1",
        "babel-plugin-css-modules-transform": "1.2.7",
        "babel-plugin-transform-assets-import-to-string": "1.0.1",
        "babel-plugin-transform-async-to-generator": "6.24.1",
        "babel-plugin-transform-class-properties": "6.24.1",
        "babel-plugin-transform-runtime": "6.23.0",
        "babel-preset-flow": "6.23.0",
        "babel-preset-react": "6.24.1",
        "babel-runtime": "6.23.0",
        "css-loader": "0.28.4",
        "extract-text-webpack-plugin": "3.0.0",
        "file-loader": "0.11.2",
        "flow-bin": "0.49.1",
        "flow-typed": "2.1.2",
        "jest": "20.0.4",
        "raw-loader": "0.5.1",
        "react-test-renderer": "15.6.1",
        "style-loader": "0.18.2",
        "webpack": "3.1.0",
        "webpack-dev-server": "2.5.1"
    },
    "jest": {
        "verbose": true,
        "testMatch": ["**/?(*.)(spec|test).js"]
    },
    "scripts": {
        "start": "webpack-dev-server --inline --devtool source-map -d",
        "storybook": "start-storybook -p 6006",
        "build": "env NODE_ENV=production webpack && cp ./src/manifest.json ./dist/manifest.json && cp ./src/asset/icon/* ./dist/ && node ./script/renameFile.js",
        "benchmark": "babel --out-dir ./dist-bench --quiet ./src && node ./dist-bench/__benchmark__/index.js 100",
        "test": "env NODE_ENV=test jest"
    }
}
